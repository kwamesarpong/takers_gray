<h1>Listing All users</h1>
<table>
	<thead>
    <tr>
    <th>Merchant ID</th>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Buy</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.national_id %></td>
        <td><a href="#">BUY</a></td>
       
      </tr>
    <% end %>
    
  </tbody>

</table>