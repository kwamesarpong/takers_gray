

<h1>Listing All Products belonging to merchant</h1>
<table>
	<thead>
    <tr>
    <th>Merchant ID</th>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @product.each do |product| %>
      <tr>
        <td><%= product.merchant_id %></td>
        <td><%= product.title %></td>
        <td><%= product.description %></td>
        <td><%= product.price %></td>
        
       
      </tr>
    <% end %>
    
  </tbody>

</table>
